<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5p [%t][%-10c] %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
		<Logger name="com.freesundance" level="info" />
		<Logger name="org.springframework" level="warn" />
		<Logger name="org.springframework.integration" level="warn" />
		<Logger name="org.springframework.integration.channel" level="warn" />
		<Logger name="org.apache.activemq" level="warn" />
		<!-- wiretap logging -->
		<Logger name="org.springframework.integration.handler" level="warn" />
	</Loggers>
</Configuration>
